<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="at.mailbox.sync.model.impl.MailboxModelImpl">
		<attributes>
			<id name="mailboxId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="description" />
			<basic name="host" />
			<basic name="port" />
			<basic name="mailboxUser" />
			<basic name="mailboxUserSecret" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="at.mailbox.sync.model.impl.TaskModelImpl">
		<attributes>
			<id name="taskId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="status" />
			<basic name="description" />
			<basic name="mailboxId" />
			<basic name="permissionUserGroupId" />
			<basic name="eventCalendarId" />
			<basic name="documentFolderId" />
			<basic name="webcontentFolderId" />
			<basic name="vocabularyId" />
			<basic name="period" />
			<basic name="clamavServerIP" />
			<basic name="clamavPort" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="at.mailbox.sync.model.impl.MailboxImpl" name="Mailbox">
		<table name="MailboxSync_Mailbox" />
		<attributes />
	</entity>
	<entity class="at.mailbox.sync.model.impl.TaskImpl" name="Task">
		<table name="MailboxSync_Task" />
		<attributes />
	</entity>
</entity-mappings>